public class Test {

	public static void main(String[] args) {
		//write a Java Main program to create the concatenation of the two strings
		String str1 = "hot", str2 = "dog";
		String result = getConcatenate (str1, str2);
		
		System.out.println("The concatenate of strings is "+ result);
		
	}
	static String getConcatenate (String str1, String str2) {
	String str3 = "";
	str3 = str1 + str2;
	return str3;
	}
}
